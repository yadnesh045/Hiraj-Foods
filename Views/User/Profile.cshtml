@model Hiraj_Foods.Models.User

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Feedback</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- google fonts -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,100i,300,300i,400,400i,500,500i,700,700i,900,900i&display=swap" rel="stylesheet">
    <link rel="shortcut icon" type="image/x-icon" href="assets\img\hiraj2.png">


    <link href="~/css/all.min.css" rel="stylesheet" />
    <link href="~/css/animate.css" rel="stylesheet" />
    <link href="~/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/icofont.min.css" rel="stylesheet" />
    <link href="~/css/lightcase.css" rel="stylesheet" />
    <link href="~/css/style.css" rel="stylesheet" />
    <link href="~/css/swiper.min.css" rel="stylesheet" />

</head>

<body>

    <!-- preloader start here -->
    <div class="preloader">
        <div class="preloader-inner">
            <div class="preloader-icon">
                <span></span>
                <span></span>
            </div>
        </div>
    </div>
    <!-- preloader ending here -->
    <!--search area-->
    <div class="search-input">
        <div class="search-close">
            <i class="fas fa-times"></i>
        </div>
        <form>
            <input type="text" name="text" placeholder="Search Heare">
            <button class="search-btn" type="submit">
                <i class="fas fa-search"></i>
            </button>
        </form>
    </div>
    <!-- search area -->
    <!-- Mobile Menu Start Here -->
    <!-- desktop menu ending here -->
    @* <section class="page-header page-header2 bg_img padding-tb">
        <div class="overlay"></div>
        <div class="container">
            <div class="page-header-content-area">
                <ul class="agri-ul">
                </ul>
            </div>
        </div>
    </section> *@
    <div class="contact-section padding-tb">
        <div class="container">
            <div class="contac-top">
                <div class="row justify-content-center">
                    <div class="col-xl-4 col-lg-6 col-12">
                        <div class="contact-item" style="border-width:2px;
                                      border-style:outset;" align="center">
                            <div class="contact-icon" style="padding-left: 15px;">
                                <i class="icofont-google-map"></i>
                            </div>
                            <div class="contact-details">
                                <p style="margin-top: 5px;"> Address -Gat No 577, Janori, <br>Tal - Dindori, dist - Nashik</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-4 col-lg-6 col-12">
                        <div class="contact-item" style="border-width:2px;
                                      border-style:outset;height: 75px;" align="center">
                            <div class="contact-icon" style="padding-left: 15px;margin-top: 10px;">
                                <i class="icofont-phone"></i>
                            </div>
                            <div class="contact-details">
                                <p style="margin-top: 15px;margin-right:45px;">+91 70306 00089</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-4 col-lg-6 col-12">
                        <div class="contact-item" style="border-width:2px;
                                      border-style:outset;height: 75px;" align="center">
                            <div class="contact-icon" style="padding-left: 15px;margin-top: 10px;">
                                <i class="icofont-envelope"></i>
                            </div>
                            <div class="contact-details">
                                <p style="margin-top: 15px;margin-right:30px;">info@hirajfoods.com</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>


            <div>

                <center><h4>User Profile</h4></center><br>
                <!--Show the bootstrap table for user data  -->
                <table class="table table-bordered table-striped">
                    <tr>
                        <th>First Name</th>
                        <td>@Model.FirstName</td>
                    </tr>
                    <tr>
                        <th>Last Name</th>
                        <td>@Model.LastName</td>
                    </tr>
                    <tr>
                        <th>Email</th>
                        <td>@Model.Email</td>
                    </tr>
                    <tr>
                        <th>Phone</th>
                        <td>@Model.Phone</td>
                    </tr>
                </table>


            </div>


        </div>
    </div>


</body>
</html>