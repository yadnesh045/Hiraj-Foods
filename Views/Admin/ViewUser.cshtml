                    </a>
                </div>

                <div class="menu-inner-shadow"></div>

                <ul class="menu-inner py-1">
                    <!-- Dashboards -->
                    <li class="menu-item">
                        <a asp-area="" asp-controller="Admin" asp-action="dashboard" class="menu-link">
