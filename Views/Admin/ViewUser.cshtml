                        <hr class="my-5" />

                        <div class="card">
                            <div class="table-responsive">
                                <table class="table table-hover">
                                    <thead>
                                        <tr>
                                            <th>FirstName</th>
                                            <th>LastName</th>
                                            <th>Email</th>
                                            <th>Password</th>
                                            <th>Phone</th>

                                        </tr>
                                    </thead>
                                    <tbody class="table-border-bottom-0">
                                        @foreach (var a in Model.Item1)
                                        {
                                            <tr>
                                                <td>@a.FirstName</td>
                                                <td>@a.LastName</td>
                                                <td>@a.Email</td>
                                                <td>@a.Password</td>
                                                <td>@a.Phone</td>
                                            </tr>

                                        }
                                        </tr>
                                    </tbody>
                                </table>

                                <!--/ Hoverable Table rows -->
