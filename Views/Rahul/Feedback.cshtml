@model Hiraj_Foods.Models.FeedBack


<!DOCTYPE html>
<html lang="en">
<head>
    <title>Feedback</title>
  

</head>

<body>

    <!-- preloader start here -->
    <div class="preloader">
        <div class="preloader-inner">
            <div class="preloader-icon">
                <span></span>
                <span></span>
            </div>
        </div>
    </div>
    <!-- preloader ending here -->
   
   
    <!-- desktop menu ending here -->
    <section class="page-header page-header2 bg_img padding-tb">
        <div class="overlay"></div>
        <div class="container">
            <div class="page-header-content-area">
                <h4 class="ph-title"><!-- Feedback Form --></h4>
                <ul class="agri-ul">
                    <!-- <li><a href="index.html">Home</a></li>
                    <li><a class="active">Contact Us</a></li> -->
                </ul>
            </div>
        </div>
    </section>

    <!-- Contact Us Section Start Here -->
    <div class="contact-section padding-tb">
        <div class="container">
            <div class="contac-top">
                <div class="row justify-content-center">
                    <div class="col-xl-4 col-lg-6 col-12">
                        <div class="contact-item" style="border-width:2px;
                                      border-style:outset;" align="center">
                            <div class="contact-icon" style="padding-left: 15px;">
                                <i class="icofont-google-map"></i>
                            </div>
                            <div class="contact-details">
                                <p style="margin-top: 5px;"> Address -Gat No 577, Janori, <br>Tal - Dindori, dist - Nashik</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-4 col-lg-6 col-12">
                        <div class="contact-item" style="border-width:2px;
                                      border-style:outset;height: 75px;" align="center">
                            <div class="contact-icon" style="padding-left: 15px;margin-top: 10px;">
                                <i class="icofont-phone"></i>
                            </div>
                            <div class="contact-details">
                                <p style="margin-top: 15px;margin-right:45px;">+91 70306 00089</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-4 col-lg-6 col-12">
                        <div class="contact-item" style="border-width:2px;
                                      border-style:outset;height: 75px;" align="center">
                            <div class="contact-icon" style="padding-left: 15px;margin-top: 10px;">
                                <i class="icofont-envelope"></i>
                            </div>
                            <div class="contact-details">
                                <p style="margin-top: 15px;margin-right:30px;">info@hirajfoods.com</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
  
            <!-- start: page -->
            <center><h4>Feedback Form</h4></center><br>
            <div class="row" style="margin-bottom: 45px;">
                <div class="col-lg-2"></div>
                <div class="col-lg-8">
                    <div class="contac-bottom" style=" border-width:2px;
                                      border-style:inset;padding-top: 15px;padding-left: 15px;padding-right: 15px;padding-bottom: 15px;">
                        <div class="row justify-content-center">
                            <div class="col-lg-12 col-12">
                                <div class="contact-form">

                                    <form method="post" asp-controller="Rahul" asp-action="Feedback" id="commentform" class="comment-form">

                                        <input type="text" name="Name" class="form-control" placeholder="Enter Name"> <br />
                                        <span asp-validation-for="Name" class="text-danger"></span>
                                        <input type="email" name="Email" class="form-control" placeholder=" Enter Email" ><br />
                                        <span asp-validation-for="Email" class="text-danger"></span>
                                        <input type="text" name="Phone" class="form-control" placeholder=" Enter Mobile Number" ><br />
                                        <span asp-validation-for="Phone" class="text-danger"></span>
                                        <textarea type="text" id="role" class="form-control" cols="30" name="Message" rows="10" placeholder="Message" ></textarea><br />
                                        <span asp-validation-for="Message" class="text-danger"></span>

                                        <button type="submit" class="lab-btn" name="submit" value="Submit">Submit</button>

                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-2"></div>
            </div>

            <!-- end: page -->
        </div>
    </div>




    <partial name="_toastr" />



</body>

</html>



