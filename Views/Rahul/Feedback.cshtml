@model Hiraj_Foods.Models.FeedBack


<!DOCTYPE html>
<html lang="en">
<head>
    <title>Feedback</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- google fonts -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,100i,300,300i,400,400i,500,500i,700,700i,900,900i&display=swap" rel="stylesheet">
    <link rel="shortcut icon" type="image/x-icon" href="assets\img\hiraj2.png">


    <link href="~/css/all.min.css" rel="stylesheet" />
    <link href="~/css/animate.css" rel="stylesheet" />
    <link href="~/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/icofont.min.css" rel="stylesheet" />
    <link href="~/css/lightcase.css" rel="stylesheet" />
    <link href="~/css/style.css" rel="stylesheet" />
    <link href="~/css/swiper.min.css" rel="stylesheet" />

</head>

<body>

    <!-- preloader start here -->
    <div class="preloader">
        <div class="preloader-inner">
            <div class="preloader-icon">
                <span></span>
                <span></span>
            </div>
        </div>
    </div>
    <!-- preloader ending here -->
    <!--search area-->
    <div class="search-input">
        <div class="search-close">
            <i class="fas fa-times"></i>
        </div>
        <form>
            <input type="text" name="text" placeholder="Search Heare">
            <button class="search-btn" type="submit">
                <i class="fas fa-search"></i>
            </button>
        </form>
    </div>
    <!-- search area -->
    <!-- Mobile Menu Start Here -->
   
    <!-- desktop menu ending here -->
    <section class="page-header page-header2 bg_img padding-tb">
        <div class="overlay"></div>
        <div class="container">
            <div class="page-header-content-area">
                <h4 class="ph-title"><!-- Feedback Form --></h4>
                <ul class="agri-ul">
                    <!-- <li><a href="index.html">Home</a></li>
                    <li><a class="active">Contact Us</a></li> -->
                </ul>
            </div>
        </div>
    </section>

    <!-- Contact Us Section Start Here -->
    <div class="contact-section padding-tb">
        <div class="container">
            <div class="contac-top">
                <div class="row justify-content-center">
                    <div class="col-xl-4 col-lg-6 col-12">
                        <div class="contact-item" style="border-width:2px;
                                      border-style:outset;" align="center">
                            <div class="contact-icon" style="padding-left: 15px;">
                                <i class="icofont-google-map"></i>
                            </div>
                            <div class="contact-details">
                                <p style="margin-top: 5px;"> Address -Gat No 577, Janori, <br>Tal - Dindori, dist - Nashik</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-4 col-lg-6 col-12">
                        <div class="contact-item" style="border-width:2px;
                                      border-style:outset;height: 75px;" align="center">
                            <div class="contact-icon" style="padding-left: 15px;margin-top: 10px;">
                                <i class="icofont-phone"></i>
                            </div>
                            <div class="contact-details">
                                <p style="margin-top: 15px;margin-right:45px;">+91 70306 00089</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-4 col-lg-6 col-12">
                        <div class="contact-item" style="border-width:2px;
                                      border-style:outset;height: 75px;" align="center">
                            <div class="contact-icon" style="padding-left: 15px;margin-top: 10px;">
                                <i class="icofont-envelope"></i>
                            </div>
                            <div class="contact-details">
                                <p style="margin-top: 15px;margin-right:30px;">info@hirajfoods.com</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
  
            <!-- start: page -->
            <center><h4>Feedback Form</h4></center><br>
            <div class="row" style="margin-bottom: 45px;">
                <div class="col-lg-2"></div>
                <div class="col-lg-8">
                    <div class="contac-bottom" style=" border-width:2px;
                                      border-style:inset;padding-top: 15px;padding-left: 15px;padding-right: 15px;padding-bottom: 15px;">
                        <div class="row justify-content-center">
                            <div class="col-lg-12 col-12">
                                <div class="contact-form">
                                    <form method="post" asp-controller="Rahul" asp-action="Feedback" id="commentform" class="comment-form">
                                        <input type="text" name="Name" class="form-control" placeholder="Enter Name" required="">
                                        <input type="email" name="Email" class="form-control" placeholder=" Enter Email" required="">
                                        <input type="text" name="Phone" class="form-control" placeholder=" Enter Mobile Number" required="">
                                        <textarea type="text" id="role" class="form-control" cols="30" name="Message" rows="10" placeholder="Message" required=""></textarea>
                                        <button type="submit" class="lab-btn" name="submit" value="Submit">Submit</button>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-2"></div>
            </div>

            <!-- end: page -->
        </div>
    </div>
    <!-- Contact Us Section ENding Here -->
    <!-- newsletters section start here -->
    <!-- <div class="newsletter-section">
      <div class="container">
        <div class="row justify-content-lg-between justify-content-center align-items-center">
          <div class="col-lg-6 col-12">
            <div class="newsletter-title">
              <h4>Subscribe Our Newsletter</h4>
            </div>
          </div>
          <div class="col-lg-6 col-12">
            <div class="newsletter-form">
              <form action="/" class="d-flex flex-wrap">
                <input type="text" placeholder="Enter Your Email" class="input-email">
                <input type="submit" value="Suibscribe" class="subscribe-btn">
              </form>
            </div>
          </div>
        </div>
      </div>
    </div> -->
    <!-- newsletters section ending here -->
    <!-- footer section start here -->
 
    <!-- footer section ending here -->
    <!-- scrollToTop start here -->
    <!-- <a href="#" class="scrollToTop"><i class="icofont-swoosh-up"></i><span class="pluse_1"></span><span class="pluse_2"></span></a> -->
    <!-- scrollToTop ending here -->
    @if (TempData["Feedback"] != null)
    {
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
        <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css">

        <script type="text/javascript">
            // Set Toastr options including custom background color
            toastr.options = {
                "toastClass": "toastr-custom", // Custom class for styling
                "closeButton": true,
                "progressBar": true,
                "positionClass": "toast-top-left",
                "progressBarColor": "white"
            };

            // Display the Toastr notification
            toastr.success('@TempData["Feedback"]');
        </script>

        <style>
            /* Custom style for Toastr notification */
            .toastr-custom {
                background-color: greenyellow !important; /* Change to your desired background color */
                color: #000000 !important;
                font-family: Cambria; /* Change to your desired text color */
            }
        </style>
    }

</body>
</html>